  <div
    id="login"
    class="relative flex-row mt-24 px-12 bg-inherit border-amber-800/50 rounded-lg animate-fade"
    phx-hook="Box"
  >
    <!-- Login Form -->
    <span class="block text-primary">welcome to the shell</span>
    <form id="boot" phx-submit="login" class="space-y-4 animate-glitchy">
      <div class="font-mono text-primary tracking-wide">
        <div class="flex items-center">
          <span class="mr-2 text-primary-content">></span>
          <span>{@session.name}</span>
          <input
            :if={!@session.name}
            type="text"
            name="username"
            class="flex-1 bg-transparent caret-current border-none focus:border-transparent focus:ring-0 outline-none text-primary placeholder-primary/80"
            placeholder="enter your name"
            autocomplete="off"
            spellcheck="false"
            phx-debounce="300"
            autofocus
          />
          <button
            :if={!@session.name}
            type="submit"
            class="absolute ml-8 -right-8 bottom-2 text-amber-400"
          >
            <svg
              xml:space="preserve"
              version="1.1"
              class="w-8 h-8 fill-primary rotate-180"
              style="shape-rendering:geometricPrecision;text-rendering:geometricPrecision;image-rendering:optimizeQuality;"
              viewBox="0 0 847 1058.75"
              x="0px"
              y="0px"
              fill-rule="evenodd"
              clip-rule="evenodd"
            >
              <defs></defs>
              <g>
                <path
                  class="fil0"
                  d="M641 641l-492 -219 492 -219 -144 222 144 216zm57 4l-80 -222 78 -222 -48 224 50 220zm-19 -3l-145 -220 145 -219 -84 220 84 219z"
                />
              </g>
            </svg>
          </button>
        </div>
      </div>
    </form>

    <!-- Decorative corners -->
